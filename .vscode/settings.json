// Place your settings in this file to overwrite default and user settings.
{
    "editor.fontSize": 20,
    "terminal.integrated.fontSize": 19,
    "editor.fontLigatures": true,
    "editor.fontFamily": "Fira Code",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/.vscode": false
    },
    "editor.formatOnSave": true
}